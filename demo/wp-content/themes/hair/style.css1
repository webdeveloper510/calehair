	/****WEBSHOP PAGE****/

	#container > div#container-heading {
	  background: none repeat scroll 0 0 #e7e7e7;
	  color: grey;
	  font-family: "Ubuntu",sans-serif;
	  font-size: 18px;
	  font-weight: bold;
	  padding: 10px;
	  text-align: center;
	}
	#container > div#l_woo {
	  background: none repeat scroll 0 0 #fff;
	}
	#l_woo h2.widget-title {
	  background: none repeat scroll 0 0 #000;
	  box-shadow: 0 2px 10px #fff inset;
	  color: white;
	  font-size: 18px;
	  font-weight: bold;
	  line-height: 38px;
	  padding: 1px;
	  margin-bottom:0;
	}
	#l_woo .product-categories {
	  background: none repeat scroll 0 0 #eee;
	  margin:0;
	} 
	#l_woo .cat-item > a {
	  color: #444;
	  line-height:40px;
	  font-weight:bold;
	  font-size:15px;
	}
	#l_woo > .secondary {
	  width: 95%;
	}
	.c_img {
	  margin: 0 !important;
	  width: 95%;
	}
	#r_woo h1.page-title {
	  display: none;
	}
	#r_woo{ 
	  background:white;
	  margin-top:10px;
	}
	.woo-shop {
	  background: none repeat scroll 0 0 #fff !important;
	}
	#r_woo .product.type-product.status-publish > a{
	  color: #000;
	}
	#r_woo ul.products li {
	  margin: 5px;
	  width: 29% !important;
	  /* border: 2px solid #ddd; */
	  padding-right:10px;
	  padding-bottom:0 !important;
	}
	#r_woo ul.products li img {
	  height: auto;
	  padding: 5px !important;
	  width: 100%;
	  padding-bottom: 0 !important;
	}
	#r_woo .product.type-product.status-publish > a h3 {
	  background: #282828;
	  color: #fff;
	  margin: 0 0 5px 5px;
	  padding: 10px;
	  width: 90%;
	}
	#r_woo .price {
	  display: none;
	}
	#r_woo .button.add_to_cart_button.product_type_simple {
	  display: none;
	}
	#r_woo .woocommerce-ordering select {
	  color: #555;
	  margin-left: 44px;
	  padding: 9px;
	  text-transform: capitalize;
	}
	
	/****SINGLE PRODUCT****/
	.woo-shop #r_woo .summary p {
	  color: #000 !important;
	}
	.woo-shop #r_woo .woocommerce-tabs ul li a{
		color: #000 !important;
	}
	.woo-shop #r_woo .woocommerce-tabs #reviews h2{
		color: #000 !important;
	}
	.woo-shop #r_woo .woocommerce-tabs #reviews p{
		color: #000 !important;
	}
	#r_woo .images .woocommerce-main-image.zoom .attachment-shop_single.wp-post-image {
	  width: 100%;
	  height: auto;
	  border-radius: 3px;
	}
	#r_woo .images {
	  border: 2px solid #BBB;
	  margin-right: 15px;
	  padding: 3px;
	  border-radius: 3px;
	}
	#r_woo .summary.entry-summary h1 {
	  float: left;
	  width: 55%;
	  border-radius: 3px;
	  text-align: left;
	  padding: 5px 12px;
	  line-height: 25px;
	}
	#r_woo .summary.entry-summary > div p {
	  margin: 0;
	  float: left;
	  width: 55%;
	  padding: 0 10px;
	  line-height: 20px;
	  text-indent: 30px;
	  text-align: justify;
	}
	#r_woo .cart {
	  float: left;
	  margin: 30px 0 100px 12px;
	  width: 100%;
	}
	#r_woo button.single_add_to_cart_button {
	  background: none repeat scroll 0 0 #ccc;
	  border: 1px solid #888;
	  border-radius: 3px;
	  color: #444;
	  font-size: 15px;
	  margin-left: 70px;
	  padding: 6px 20px;
	}
	#r_woo button.submit {
	  background: none repeat scroll 0 0 #ccc;
	  border: 1px solid #888;
	  border-radius: 3px;
	  color: #444;
	  font-size: 15px;
	  margin-left: 70px;
	  padding: 6px 20px;
	}
	#r_woo .quantity {
	  margin: 0 0 10px 50px;
	}
	.panel.entry-content {
	  background: none repeat scroll 0 0 #ddd;
	  border-radius: 3px;
	  font-family: ubuntu;
	  padding: 5px 35px;
	}
	.woocommerce-tabs .submit {
	  background: #333;
	  color: #EEE;
	  padding: 5px 10px;
	  border: none;
	  font-size: 15px;
	  border-radius: 3px;
	  margin-left: 405px;
	}
	.stars {
	  float: left;
	  margin: -15px 0 0 15px;
	}
	a.star-1, a.star-2, a.star-3, a.star-4, a.star-5 {
	  color: #fff;
	  padding: 5px 9px;
	  text-decoration: none;
	  background: #333;
	  margin-right: 2px;
	  border-radius: 12px;
	}
	
	/****CONTACT PAGE****/
	section#contact {
		background-color: #000;
		/* background-image: url(../images/cont.png); */
		background-position: center;
		background-repeat: no-repeat;
		padding-top: 30px;
		/*opacity: 0.6; */
		background-size: cover;
	}

	section#contact .section-heading {
		color: #fff;
		margin-bottom: 30px;
	}

	section#contact .form-group {
		margin-bottom: 25px;
	}

	section#contact .form-group input,
	section#contact .form-group textarea {
		padding: 20px;
	}

	section#contact .form-group input.form-control {
		height: auto;
	}

	section#contact .form-group textarea.form-control {
		height: 236px;
	}

	section#contact .form-control:focus {
		border-color: #277fa8;
		box-shadow: none;
	}

	section#contact::-webkit-input-placeholder {
		text-transform: uppercase;
		font-family: Montserrat,"Helvetica Neue",Helvetica,Arial,sans-serif;
		font-weight: 700;
		color: #bbb;
	}

	section#contact:-moz-placeholder {
		text-transform: uppercase;
		font-family: Montserrat,"Helvetica Neue",Helvetica,Arial,sans-serif;
		font-weight: 700;
		color: #bbb;
	}

	section#contact::-moz-placeholder {
		text-transform: uppercase;
		font-family: Montserrat,"Helvetica Neue",Helvetica,Arial,sans-serif;
		font-weight: 700;
		color: #bbb;
	}

	section#contact:-ms-input-placeholder {
		text-transform: uppercase;
		font-family: Montserrat,"Helvetica Neue",Helvetica,Arial,sans-serif;
		font-weight: 700;
		color: #bbb;
	}

	section#contact .text-danger {
		color: #e74c3c;
	}
	
	/****HOMEPAGE****/
	.black-anchor{ color: #000 !important; text-decoration:none;}
	
	/****VERZORING****/
	#wrapper > div#leftcon.verzor,
	#wrapper > div#leftcon.b_after,
	#wrapper > div#leftcon.prijzen {
	  background: none repeat scroll 0 0 #fff;
	}
	#leftcon.verzor h2.widget-title,
	#leftcon.b_after h2.widget-title,
	#leftcon.prijzen h2.widget-title {
	  background: none repeat scroll 0 0 #000;
	  box-shadow: 0 2px 10px #fff inset;
	  color: white;
	  font-size: 18px;
	  font-weight: bold;
	  line-height: 38px;
	  padding: 1px;
	  margin-bottom:0;
	}
	#leftcon.verzor .product-categories,
	#leftcon.b_after .product-categories,
	#leftcon.prijzen .product-categories {
	  background: none repeat scroll 0 0 #eee;
	  margin:0;
	} 
	#leftcon.verzor .cat-item > a,
	#leftcon.b_after .cat-item > a,
	#leftcon.prijzen .cat-item > a {
	  color: #444;
	  line-height:40px;
	  font-weight:bold;
	  font-size:15px;
	}
	#leftcon.verzor > .secondary,
	#leftcon.b_after > .secondary,
	#leftcon.prijzen > .secondary {
	  width: 95%;
	}
	.secondary  nav { display:none !important;}
	.woocommerce-tabs {display:none !important;}
	
	.term-description {
	  float: right;
	  margin-top: 20px;
	  padding: 0;
	  position: relative;
	  width: 62%;
	} 
	.term-description > p {
	  margin: 0;
	  padding: 0;
	  width: 100%;
	}
	
