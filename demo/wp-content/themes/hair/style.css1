 /* CSS Document */

#wrapper {
	background-color: $FFFFFF;
	margin: 0 auto;
	width: 960px;
	}
	
#webshop {
	width: 800px;
	padding-left: 80px;
	padding-right: 80px;
	padding-top: 10px;
	padding-bottom: 10px;
}

#header {
	background-color: #000000;
	height: 70px;
	}

.Intro {
	weight: 900px;
	color: #282828;
	background-color: #E7E7E7;
	padding-top: 10px;
	margin: 0 auto;
	height: 30px;
	font-size: 14px;
	font-family: 'Ubuntu', sans-serif;
	text-align: center;
}
	
#nav ul {
	width: 900px;
	margin: 0 auto;
	height: 35px;
	text-decoration: none;
	padding-top: 25px;
	padding-bottom: 20px;
}
#nav ul li {
	display: inline;
	padding-left: 36px;
	padding-right: 36px;
	width: 880px;
}

ul li a {
	text-decoration: none;
	color: #FFFFFF;
	font-family: arapey;
	font-style: normal;
	font-weight: 400;
	font-family: 'Ubuntu', sans-serif;
	font-size: 14px;
}

#nav   {
	height: 30px;
}

h1 {
	color: #282828;
	background-color: #E7E7E7;
	margin-top: 10px;
	margin-bottom: 10px;
	padding-left: 80px;
	height: 30px;
	font-size: 14px;
	font-family: 'Ubuntu', sans-serif;
	padding-top: 10px;
}
.balk {
	background-color: #E7E7E7;
	height: 30px;
	width: 960px;
	color: #777777;
	font-family: arapey;
	font-size: 14px;
	text-align: center;
	padding-top: 10px;
	font-family: 'Ubuntu', sans-serif;
}
.zwartbalk {
	height: 30px;
	background-color: #282828;
}
#slideshow {
	height: 490px;
}

p {
	width: 780px;
	padding-left: 80px;
	padding-right: 80px;
	font-family: 'Ubuntu', sans-serif;
	font-size: 14px;
	margin-bottom:10px;
	color: #282828;
}

.leftcol {
	float: left;
	padding-right: 10px;
	width:32%;
}
.midcol {
	float: left;
	padding-right: 7px;
}

.rightcol {
	float: right;
	}
	
.submenu {
	color: #FFFFFF;
	background-color: #282828;
	height: 30px;
	font-size: 14px;
	font-family: 'Ubuntu', sans-serif;
	padding-top: 10px;
	text-align: center;
}
h2.submenu a {
color: #fff;
text-decoration: none;
}
.kop {
	weight: 900px;
	color: #282828;
	background-color: #E7E7E7;
	padding-top: 10px;
	margin: 0 auto;
	height: 30px;
	font-size: 14px;
	font-family: 'Ubuntu', sans-serif;
	text-align: center;
	}
	
.selfies {
	padding-top: 10px;
	padding-bottom: 10px;
}

#footer {
	background-color: #000000;
	margin-top: 20px;
	height: 75px;
}

#footer ul {
	width: 800px;
	margin-left: 60px;
	margin-right: 80px;
	margin: 0 auto;
	text-decoration: none;
	padding-top: 25px;
	padding-bottom: 20px;
}

#footer ul li {	
	text-align: center;
	padding-right: 15px;
	display: inline;
	color: #FFFFFF;
	font-family: 'Ubuntu', sans-serif;
	font-size: 10px;	
	display: inline;	
}
.woocommerce-result-count { display:none; }
 .woocommerce-breadcrumb { display:none;  }
 #container { width:100%; background-color:#000 }
 .widget-title {  background-color:#E7E7E7;min-height: 40px !important;
margin-top: 10px; border-right: 1px solid #000;  }
 #l_woo { float:left;width:24% }
 #r_woo { width: 75%; overflow: hidden;min-height: 866px;}
ul.product-categories li { list-style:none; }
ul.product-categories li a {font-size: 16px;line-height: 30px; }
ul.products li { list-style:none; float:left; width:32%; padding-bottom: 10px;border: 1px solid #fff; text-align: center; }
ul.products li img { padding-top:10%; }
a.add_to_cart_button { color:red; }
.onsale { display:none;}
.images { float:left;width:38%; }
.summary p { color:#fff;width: 88%; text-align: left;}
.single_add_to_cart_button { color:chocolate; }
.summary  .sku_wrapper { display:none; }
.summary  .posted_in { display:none; }
.woocommerce-message a { color: bisque; font-size: 20px; }
del { display:none}
#reviews h2 { color:#fff; }
#reviews p { color:#fff; }
a.star-1, a.star-2,a.star-3, a.star-4, a.star-5  { color:#fff;padding:5px; }
.comment-form-comment label { float:left;padding:10px; } 
img.c_img { margin-left: 10%; }
h2.widget-title {    text-align: center; color: #282828; font-size: 16px;font-family: 'Ubuntu', sans-serif; }
.w_p { background-color:#fff;min-height: 7px  }

